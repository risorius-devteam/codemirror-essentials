{
  "name": "highlight-example",
  "root": "examples/highlight",
  "sourceRoot": "examples/highlight/src",
  "projectType": "application",
  "tags": ["type:app", "framework:react", "scope:example"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite build",
        "cwd": "examples/highlight"
      },
      "outputs": ["{projectRoot}/dist"],
      "cache": true,
      "dependsOn": ["react-codemirror:build"]
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite",
        "cwd": "examples/highlight"
      },
      "cache": false
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite preview",
        "cwd": "examples/highlight"
      },
      "cache": false,
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["examples/highlight/**/*.{ts,tsx,js,jsx}"]
      },
      "cache": true
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit",
        "cwd": "examples/highlight"
      },
      "cache": true
    }
  }
}